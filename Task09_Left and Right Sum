#include <iostream>

int main()
{
	int n;
	std::cin >> n;

	int sum1 = 0;
	int sum2 = 0;

	for (int i = 0; i < n; i++)
	{
		int num;
		std::cin >> num;
		
		sum1 +=num;
	}
	
	for (int i = 0; i < n; i++)
	{
		int num;
		std::cin >> num;
		sum2 += num;
	}

	int diff = abs(sum1 - sum2);

	if (diff ==0)
	{
		std::cout << "Yes, sum = " << sum1 << std::endl;

	}
	else
	{
		std::cout << "No, diff = " << abs(sum1 - sum2) << std::endl;
	}

	return 0;
}
